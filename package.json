{
  "name": "srs",
  "version": "0.4.4",
  "main": "./lib/index.js",
  "description": "Spatial reference library for node",
  "keywords": [
    "map",
    "projection",
    "spatialreference",
    "srid",
    "proj4",
    "mercator",
    "gdal",
    "ogr",
    "osr"
  ],
  "binary": {
    "module_name"  : "srs",
    "module_path"  : "./lib/binding/{node_abi}-{platform}-{arch}",
    "remote_path"  : "./{name}/v{version}/",
    "package_name" : "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz",
    "host"         : "https://mapbox-node-binary.s3.amazonaws.com"
  },
  "url": "http://github.com/mapbox/node-srs",
  "repository": {
    "type": "git",
    "url": "git://github.com/mapbox/node-srs.git"
  },
  "dependencies": {
    "nan": "~1.4.1",
    "node-pre-gyp": "https://github.com/mapbox/node-pre-gyp/tarball/master"
  },
  "bundledDependencies": [
    "node-pre-gyp"
  ],
  "author": "Dane Springmeyer <dane@mapbox.com>",
  "licenses": [
    "BSD"
  ],
  "engines": {
      "node": ">= 0.8.0 < 0.13"
  },
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build",
    "test": "mocha -R spec"
  },
  "devDependencies": {
    "mocha": "1.x",
    "aws-sdk": "~2.0.25"
  }
}
